RewriteEngine On
RewriteBase     /new
RewriteRule ^index\.php$ - [L]
RewriteRule (css|images|js)/(.*)  $1/$2 [L,QSA]


RewriteRule  new/wadmin/index.php/(.*)          new/wadmin/index.php/$1 [L]
RewriteRule  wadmin/(.*)          wadmin/$1 [L]

RewriteCond %{QUERY_STRING} page=order$ [NC]
RewriteRule .* index.php?module=order [L,QSA]

RewriteCond %{QUERY_STRING} page=orderdetails(.*)$ [NC]
RewriteRule .* index.php?module=order&task=orderdetails [L,QSA]

RewriteCond %{QUERY_STRING} page=order&page2=1$ [NC]
RewriteRule .* index.php?module=order&task=userorder [L,QSA]

RewriteCond %{QUERY_STRING} page=order&page2=2$ [NC]
RewriteRule .* index.php?module=order&task=addinorder [L,QSA]

RewriteCond %{QUERY_STRING} page=(.*)$ [NC]
RewriteRule .* index.php?module=shopcoins&task=%1 [L,QSA]

RewriteCond %{QUERY_STRING} term=(.*)$ [NC]
RewriteRule .* index.php?module=shopcoins&task=searchajax [L,QSA]

RewriteCond %{QUERY_STRING} search=revaluation [NC]
RewriteRule .* index.php?module=shopcoins&task=shopcoins [L,QSA]
RewriteCond %{QUERY_STRING} search=newcoins [NC]
RewriteRule .* index.php?module=shopcoins&task=shopcoins [L,QSA]


RewriteCond %{QUERY_STRING} search=(.*)$ [NC]
RewriteRule .* index.php?module=shopcoins&task=search [L,QSA]

#RewriteCond %{REQUEST_URI}	^/shopcoins/.*_pp([0-9]*)\.html
RewriteRule shopcoins/.*_c([0-9]*)_pc([0-9]*)_m([0-9]*)_pp([0-9]*)\.html$ index.php?module=shopcoins&task=show&catalog=$1&parent=$2&materialtype=$3&pagenumparent=$4 [L]
#RewriteCond %{REQUEST_URI}	^/shopcoins/.*_m([0-9]*)\.html
RewriteRule shopcoins/.*_c([0-9]*)_m([0-9]*)\.html$ index.php?module=shopcoins&task=show&catalog=$1&materialtype=$2 [L] 


RewriteRule  shopcoins/activcoupon.php          index.php?module=order&task=activcoupon [L,QSA]
RewriteRule  shopcoins/submitorder.php          index.php?module=order&task=submitorder [L,QSA]
RewriteRule  shopcoins/postcalculate.php          index.php?module=order&task=postcalculate [L,QSA]
RewriteRule  shopcoins/paymentdescription.php     index.php?module=delivery&task=paymentdescription [L,QSA]
RewriteRule  shopcoins/deliverydescription.php    index.php?module=delivery&task=deliverydescription [L,QSA]
RewriteRule  shopcoins/showallmetro.php           index.php?module=delivery&task=showallmetro [L,QSA]
RewriteRule  shopcoins/showinoffice.php           index.php?module=delivery&task=showinoffice [L,QSA]
RewriteRule  shopcoins/showringmetro.php          index.php?module=delivery&task=showringmetro [L,QSA]


RewriteRule  shopcoins/change_coin.php    index.php?module=shopcoins&task=change_coin [L,QSA]
RewriteRule  shopcoins/addsubscribecatalog.php    index.php?module=shopcoins&task=addsubscribecatalog [L,QSA]
RewriteRule  shopcoins/addnext.php    index.php?module=shopcoins&task=addnext [L,QSA]
RewriteRule  shopcoins/addbascetlast.php    index.php?module=shopcoins&task=addbascetlast [L,QSA]
RewriteRule  shopcoins/detailscoins/addname.php    index.php?module=detailscoins&task=addname [L,QSA]
RewriteRule  shopcoins/addreview.php     index.php?module=shopcoins&task=addreview [L,QSA]
RewriteRule  shopcoins/addmark.php     index.php?module=shopcoins&task=addmark [L,QSA]
RewriteRule  shopcoins/addbascet.php     index.php?module=shopcoins&task=addbascet [L,QSA]
RewriteRule  shopcoins/addoneklick.php     index.php?module=shopcoins&task=addoneklick [L,QSA]
RewriteRule  shopcoins/?(.*)               index.php?module=shopcoins&$1 [L,QSA]
RewriteRule  shopcoins/                    index.php?module=shopcoins
RewriteRule (.*)/(.*).php$                 index.php?module=$1&task=$2 [L,QSA]
RewriteRule (.*).php$                      index.php?module=$1 [L,QSA]
RewriteRule (.*)$                          index.php?module=$1 [L,QSA]